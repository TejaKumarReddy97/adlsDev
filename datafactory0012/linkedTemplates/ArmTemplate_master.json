{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory0012"
		},
		"ls_adls_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_gen2'"
		},
		"ls_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa'"
		},
		"ls_adls_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devstradls.dfs.core.windows.net/"
		},
		"ls_adls_gen2_kv_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'ls_adls_gen2_kv'"
		},
		"ls_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_kv_01_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-new-01.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory0012_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_gen2_accountKey": {
						"value": "[parameters('ls_adls_gen2_accountKey')]"
					},
					"ls_sa_connectionString": {
						"value": "[parameters('ls_sa_connectionString')]"
					},
					"ls_adls_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_gen2_properties_typeProperties_url')]"
					},
					"ls_adls_gen2_kv_sasUri": {
						"value": "[parameters('ls_adls_gen2_kv_sasUri')]"
					},
					"ls_http_properties_typeProperties_url": {
						"value": "[parameters('ls_http_properties_typeProperties_url')]"
					},
					"ls_kv_01_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_01_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}